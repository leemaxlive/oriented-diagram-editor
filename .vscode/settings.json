// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "typescript.tsdk": "node_modules/typescript/lib",
    "emmet.syntaxProfiles": {
        "typescript":"html"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.js":{ "when": "$(basename).ts" },
        "**/*.js.map":true
    }
}